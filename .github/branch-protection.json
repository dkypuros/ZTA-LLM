{
  "required_status_checks": {
    "strict": true,
    "contexts": [
      "Security Validation / Security Fixes Validation",
      "Security Validation / Container Vulnerability Scanning",
      "Security Validation / Secret Detection",
      "Security Validation / OPA Policy Testing",
      "Security Validation / Kubernetes Manifest Validation"
